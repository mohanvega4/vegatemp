# Local Development Environment Configuration

# Node Environment
NODE_ENV=development
PORT=5000

# Database Configuration - Local PostgreSQL
DATABASE_URL=postgres://postgres:postgres@localhost:5432/vegashow
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=vegashow

# AWS RDS Configuration (not used in local environment but kept for compatibility)
AWS_RDS_HOST=localhost
AWS_RDS_PORT=5432
AWS_RDS_DATABASE=vegashow
AWS_RDS_USERNAME=postgres
AWS_RDS_PASSWORD=postgres

# Session Configuration
SESSION_SECRET=local_development_session_secret_key

# Application Settings
LOG_LEVEL=debug
ENABLE_SWAGGER=true