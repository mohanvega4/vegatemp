# Production Environment Configuration

# Node Environment
NODE_ENV=production
PORT=5000

# Database Configuration - AWS RDS
DATABASE_URL=postgres://username:password@your-production-instance.region.rds.amazonaws.com:5432/vegashow
AWS_RDS_HOST=your-production-instance.region.rds.amazonaws.com
AWS_RDS_PORT=5432
AWS_RDS_DATABASE=vegashow
AWS_RDS_USERNAME=username
AWS_RDS_PASSWORD=password

# Session Configuration
SESSION_SECRET=production_session_secret_change_me

# CORS Configuration
ALLOWED_ORIGINS=https://vegashow.ai,https://www.vegashow.ai

# Application Settings
LOG_LEVEL=info
ENABLE_SWAGGER=false

# SSL Configuration
SSL_ENABLED=true